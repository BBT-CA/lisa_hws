#ifndef LOCATOR_H_
#define LOCATOR_H_
class Iterator;
class Data;
class Locator {
private :
	Data key;

public:
//	virtual Locator() = NULL;
	virtual Iterator& locate(Data key) =NULL;
	virtual ~Locator();

};

#endif /* LOCATOR_H_ */

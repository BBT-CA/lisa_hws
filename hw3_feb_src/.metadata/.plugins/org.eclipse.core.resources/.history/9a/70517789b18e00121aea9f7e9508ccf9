#include "Sorted.h"

Sorted::Sorted() {}

Sorted::~Sorted() {}
Iterator Sorted::locate( Data key)
{

	  Iterator iter= createIterator();
     //while (!(node->getNext()->getData().get ()))  // already finish travel
      while (iter.hasNext())
	  {
 	     if(key.get() >= iter.get())
 	             {
				 iter.next();
				}
	  }
 	        return iter;

}
